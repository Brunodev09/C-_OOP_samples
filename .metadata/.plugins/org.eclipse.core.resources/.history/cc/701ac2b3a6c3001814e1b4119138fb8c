#include <iostream>
#include "class.h"
using namespace std;

void Person::setAge(int age) {
		if (age == 0) return;
		else if (age < 0) this->age = 0;
		else if (age > 130) this->age = 130;
		else this->age = age;
//.this refers
}

int Person::getAge() {
	return age;
}




