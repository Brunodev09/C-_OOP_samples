#include <iostream>
#include "integer.h"

using namespace std;

Integer::Integer() {
	cout << endl << "Convert constructor invoked!";
}

Integer::Integer(int num) {
	this->num = num;
}

int Integer::operator +=(Integer o) {
	return 50;
}

int Integer::getNum() {
	return num;
}

Integer::operator int() {
	return num;
}

Integer::~Integer() {
	cout << endl << "Destructor invoked!";
}



