

#ifndef TEMP_H_
#define TEMP_H_

#include <iostream>

using namespace std;

template<typename P>//This creates the template of a class. Definitions of func must be on the same file

class Point {
protected:
	P x;
public:
	Point(P =0);
	~Point();
	P getX();
	void setX(int);
};
template<typename P>
Point<P>::Point(P x) {
	this->x = x;
}
template<typename P>
int Point<P>::getX() {
	return x;
}
template<typename P>
void Point<P>::setX(int x) {
	this->x = x;
	cout << "I'm from Point!" << endl;
}
template<typename P>
Point<P>::~Point() {

}


