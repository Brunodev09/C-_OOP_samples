#include <iostream>
#include "copy.h"

using namespace std;

Copy::Copy(int x, int y, int value) {
	this->x = x;
	this->y = y;
	this->p = new int[value];//pointer already declared
}

Copy::~Copy() {
	cout << "Destructor initialized!" << endl;
}




