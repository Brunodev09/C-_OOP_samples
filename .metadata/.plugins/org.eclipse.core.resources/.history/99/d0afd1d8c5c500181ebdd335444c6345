#include <iostream>
#include "farm.h"

using namespace std;

Animal::Animal() {
	voice = "default";
}

string Animal::getVoice() {
	return voice;
}

Animal::~Animal() {

}

Dog::Dog() {
	voice = "woof woof" + ID;
}

Dog::~Dog() {

}

Cat::Cat() {
	voice = "meow meow";
}

Cat::~Cat() {

}

Cow::Cow() {
	voice = "mooh mooh";
}

Cow::~Cow() {

}




