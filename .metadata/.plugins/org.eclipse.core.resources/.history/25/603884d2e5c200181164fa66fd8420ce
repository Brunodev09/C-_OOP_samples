#include <iostream>

using namespace std;

void random(int, int);

int main() {

	random(10,6);
	return 0;
}

void random(int range, int total) {
	if (range < total) return;

	srand(time(NULL));
	int *draw = new int[total]; //to use a dynamic array, just use a pointer

	for (int i = 0; i < total; i++) {
		draw[i] = rand() % range + 1;
		cout << draw[i] << endl;
	}

	delete []draw;
}
