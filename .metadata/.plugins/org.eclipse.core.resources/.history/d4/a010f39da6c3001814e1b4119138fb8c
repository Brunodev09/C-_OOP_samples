#include <iostream>
#include "class.h"
using namespace std;

void Person::setAge(int age) {
		if (this->age == 0) return;
		else if (this->age < 0) age = 0;
		else if (this->age > 130) age = 130;
		else age = this->age;

}

int Person::getAge() {
	return age;
}




