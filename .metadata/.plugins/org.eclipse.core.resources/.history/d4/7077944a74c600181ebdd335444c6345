#include <iostream>
#include "exception.h"

using namespace std;

double division(double a, double b) {
	return a/b;
}

int main() {


	try {
		division(5,0);
	} catch (int error) {
		cout << "The variable cannot be equal to " << a << endl;
	} catch (...) {
		cout << "This is like an else statement. It will get every other types other than int" << endl;
	}

	return 0;
}

