#include <iostream>

using namespace std;

class Person {
private:
	short age = 0;
public:
	void setAge(int value) {age = value;}
	short getAge() {return age;}
};

int main() {
	Person person;
//	cout << person.age << endl;
	short myAge = person.getAge();
	cout << myAge << endl;
	person.setAge(21);
	return 0;
}
